{
  "name": "express-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npx tsc ./server/index.ts --esModuleInterop --outDir ./dist --watch\" \"nodemon -q dist/index.js\"",
    "build": "next build && npm run prisma:generate && npx tsc ./server/index.ts --esModuleInterop --outDir ./dist",
    "prisma:generate": "prisma generate --schema=./server/prisma/schema.prisma",
    "start": "NODE_ENV=production node dist/index.js",
    "lint": "next lint"
  },
  "dependencies": {
    "@prisma/client": "^3.15.2",
    "@reduxjs/toolkit": "^1.8.2",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "next": "12.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.2",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.13",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/socket.io": "^3.0.2",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.2",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "nodemon": "^2.0.16",
    "postcss": "^8.4.14",
    "prisma": "^3.15.2",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.7.4"
  }
}
